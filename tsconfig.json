{
	"compilerOptions": {
		"target": "es6",
		"module": "node16",
		"strict": true,
		"paths": {
			"@root/*": [
				"./*"
			],
			"@src/*": [
				"./src/*"
			],
			"@tempVC/*": [
				"./src/functions/tempVoiceChannels/*"
			],
			"@meme/*": [
				"./src/functions/meme/*"
			],
		}
	},
	"include": [
		"./src"
	],
	"exclude": [
		"./node_modules",
		"./src/**/*.ignore.ts"
	]
}